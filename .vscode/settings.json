{
    "search.exclude": {
        "**/node_modules": false
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.formatOnSave": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "eslint.enable": true,
    "eslint.run": "onType",
    "eslint.debug": true,
    "eslint.useFlatConfig": true,
    "typescript.tsdk": "node_modules/typescript/lib"
}